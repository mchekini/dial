node{


    stage('checkout'){
        checkout([$class: 'GitSCM', branches: [[name: "*/$branch"]], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/mchekini/dial.git']]])
    }
    stage('build'){
        sh 'cd ./back'
        sh 'chmod +x gradlew'
    }
}